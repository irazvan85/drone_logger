# Frontend Environment Variables
# Copy this file to .env.local and fill in the values

# API Configuration
VITE_API_URL=http://localhost:8000/api
VITE_API_VERSION=v1

# Development
VITE_DEBUG=false

# Map Configuration
VITE_MAP_DEFAULT_LAT=40
VITE_MAP_DEFAULT_LNG=-95
VITE_MAP_DEFAULT_ZOOM=4
VITE_MAP_TILE_PROVIDER=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_TILE_ATTRIBUTION=&copy; OpenStreetMap contributors
